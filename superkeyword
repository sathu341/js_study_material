       class Person{
        constructor(firstname,lastname){
            this.fname=firstname
            this.lname=lastname
        }
       }
       class Employee extends Person{
        
        constructor(department,designation,fname,lname){
            super(fname,lname)
            this.dep=department
            this.des=designation 

        }
        display(){
           console.log(this.dep,this.des)
           console.log(this.fname,this.lname)
           
        }
       }
       let ram=new Employee('it',"developer",'ram','krishna')
       ram.display()
     </script>
